{
  "arrowParens": "avoid",
  "generatorStarSpacing": true,
  "indentChains": true,
  "offsetTernaryExpressions": true,
  "printWidth": 120,
  "quoteProps": "consistent",
  "semi": false,
  "singleQuote": true,
  "spaceBeforeFunctionParen": true,
  "yieldStarSpacing": true,
  "overrides": [
    {
      "files": [".eslintrc.json"],
      "options": {
        "printWidth": 80
      }
    }
  ]
}
